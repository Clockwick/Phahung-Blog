{
  "name": "@phahung/phahung-admin-app",
  "version": "0.0.0",
  "private": true,
  "prettier": "@phahung/prettier-config",
  "scripts": {
    "pre-commit": "lint-staged",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "rustywind --write",
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|md)": [
      "prettier --write"
    ],
    "*.html": [
      "rustywind --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@chan-chala/uikit": "0.0.0",
    "@editorjs/code": "^2.7.0",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/editorjs": "^2.22.2",
    "@editorjs/embed": "^2.4.6",
    "@editorjs/header": "^2.6.2",
    "@editorjs/image": "^2.6.1",
    "@editorjs/inline-code": "^1.3.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/paragraph": "^2.8.0",
    "@editorjs/quote": "^2.4.0",
    "@editorjs/underline": "^1.0.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@reduxjs/toolkit": "^1.6.1",
    "@tailwindcss/line-clamp": "^0.2.2",
    "axios": "^0.23.0",
    "codex-tooltip": "^1.0.4",
    "editorjs-blocks-react-renderer": "^1.2.4",
    "editorjs-drag-drop": "^0.1.3",
    "editorjs-strikethrough": "^1.0.0",
    "editorjs-text-alignment-blocktune": "^1.0.2",
    "firebase": "^9.6.3",
    "firebase-admin": "^10.0.2",
    "formik": "^2.2.9",
    "framer-motion": "^4",
    "moment": "^2.29.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-editor-js": "^1.10.0",
    "react-redux": "^7.2.5",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.3.0",
    "react-share": "^4.4.0",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@phahung/eslint-config": "^1.0.0",
    "@phahung/prettier-config": "^1.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.3.4",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "rustywind": "^0.12.2",
    "tailwindcss": "^2.2.9",
    "typescript": "^4.3.2",
    "vite": "^2.5.2",
    "vite-plugin-eslint": "^1.3.0",
    "vite-tsconfig-paths": "^3.3.13"
  }
}
