{
  "name": "@chan-chala/uikit",
  "version": "0.0.0",
  "main": "./src/index.tsx",
  "private": true,
  "prettier": "@chan-chala/prettier-config",
  "scripts": {
    "pre-commit": "lint-staged",
    "dev": "yarn storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "rustywind --write",
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|md)": [
      "prettier --write"
    ],
    "*.html": [
      "rustywind --write"
    ]
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-switch": "^6.0.0",
    "react-tooltip": "^4.2.21"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@phahung/eslint-config": "^1.0.0",
    "@phahung/prettier-config": "^1.0.0",
    "@storybook/addon-actions": "^6.4.0-alpha.33",
    "@storybook/addon-essentials": "^6.4.0-alpha.33",
    "@storybook/addon-links": "^6.4.0-alpha.33",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.0-alpha.33",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.9",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "rustywind": "^0.12.2",
    "storybook-builder-vite": "^0.0.12",
    "tailwindcss": "^2.2.9",
    "typescript": "^4.3.2",
    "vite": "^2.5.2",
    "vite-plugin-eslint": "^1.3.0"
  }
}
